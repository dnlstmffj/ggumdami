<body >
<!-- Site wrapper -->

  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
      <h1>
        주제선택프로그램
        <small>수강과정 정보</small>
      </h1>
      <ol class="breadcrumb">
        <li class="active"><a href="#"><i class="fa fa-dashboard"></i> 새 수강과정</a></li>


      </ol>
    </section>

    <!-- Main content -->
    <section class="content">

      
      <div class="box box-warning">
        <div class="box-header with-border">
          <h3 class="box-title">기본 정보</h3>
        </div>
        <!-- /.box-header -->
        <div class="box-body">
          <form role="form">
            <!-- text input -->
            <div class="form-group">
              <label>수강과정명</label>
              <input type="text" class="form-control" placeholder="예술체육프로그램">
              <br>
              <label>수강과정 아이디</label>
              <input type="text" class="form-control" placeholder="artsport">
            </div>
            <div class="form-group">
                <label>기수 개수</label>
                <select class="form-control">
                  <option>1개 기수</option>
                  <option>2개 기수</option>
                  <option>3개 기수</option>
                  <option>4개 기수</option>
                  <option>5개 기수</option>
                </select>
              </div>
          </form>
        </div>
        <div class="box-footer">
            <button type="submit" class="btn btn-primary">저장</button>
          </div>
        <!-- /.box-body -->
      </div>
      <div class="box box-warning">
          <div class="box-header with-border">
            <h3 class="box-title">1기 정보</h3>
          </div>
          <!-- /.box-header -->
          <div class="box-body">
            <form role="form">
              <!-- text input -->
              <div class="form-group">
                <label>Date range:</label>

                <div class="input-group">
                  <div class="input-group-addon">
                    <i class="fa fa-calendar"></i>
                  </div>
                  <input type="text" class="form-control pull-right" id="reservation">
                </div>
                <!-- /.input group -->
              </div>
              <div class="form-group">
                  <label>프로그램</label>
                  <select class="form-control select2" multiple="multiple" data-placeholder="Select a State" style="width: 100%;">
                    <option>과학탐구반</option>
                    <option>영화감상반</option>
                    <option>독도사랑반</option>

                  </select>
                </div>
            </form>
          </div>
          <div class="box-footer">
              <button type="submit" class="btn btn-primary">저장</button>
            </div>
          <!-- /.box-body -->
        </div>
      <!-- /.box -->
    </section>
    <!-- /.content -->
  </div>
  <!-- /.content-wrapper -->
  <!-- /.control-sidebar -->
  <!-- Add the sidebar's background. This div must be placed
       immediately after the control sidebar -->

